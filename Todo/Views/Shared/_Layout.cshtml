<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Tareas</title>
    <link rel="stylesheet" href="~/Content/assets/css/material-design-icons.css" type="text/css" />    
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/assets/css/font.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/assets/css/app.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/font-awesome.css" />
</head>
<body ng-cloak ng-app="App">
    <div class="app">
        <div id="content" class="app-content" role="main">
            <div class="box">
                @Html.Partial("_Header")

                <div class="box-row">
                    <div class="box-cell">
                        <div class="box-inner padding">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Content/assets/js/jquery.js"></script>
    <script src="~/Content/assets/js/ui-load.js"></script>
    <script src="~/Content/assets/js/ui-jp.config.js"></script>
    <script src="~/Content/assets/js/ui-jp.js"></script>
    <script src="~/Content/assets/js/ui-nav.js"></script>
    <script src="~/Content/assets/js/ui-toggle.js"></script>
    <script src="~/Content/assets/js/ui-form.js"></script>
    <script src="~/Content/assets/js/ui-client.js"></script>
    <script src="~/Content/assets/js/bootstrap.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-sanitize.min.js"></script>
    <script src="~/Scripts/App/Main/App.js?v=0.0.3"></script>
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
    @RenderSection("scripts", required: true)
</body>
</html>
